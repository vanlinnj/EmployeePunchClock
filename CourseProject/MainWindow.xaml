<Window x:Class="CourseProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CourseProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid x:Name="MainScreen">
        <TextBox x:Name="txtBoxEmployeeID" HorizontalAlignment="Left" Height="23" Margin="248,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="employeeIDLabel" Content="Employee ID:" HorizontalAlignment="Left" Margin="146,90,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label" Content="CIS 310 Punch Clock" HorizontalAlignment="Left" Margin="199,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnLogIn" Content="LogIn" HorizontalAlignment="Left" Margin="278,179,0,0" VerticalAlignment="Top" Width="75" Click="btnLogIn_Click"/>
        <Button x:Name="btnPunch" Content="Punch" HorizontalAlignment="Left" Margin="169,179,0,0" VerticalAlignment="Top" Width="75" Click="btnPunch_Click"/>
        <Button x:Name="btnListEmployees" Content="List Employees" HorizontalAlignment="Left" Margin="43,179,0,0" VerticalAlignment="Top" Width="97" Click="btnListEmployees_Click"/>

    </Grid>
</Window>
